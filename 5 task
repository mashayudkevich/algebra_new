#include <stdio.h>
#include <stdlib.h>
int main(){
int i,n,flag;
printf("number of elements in the permutation\n");
scanf("%d",&flag);
printf("number of elements in the permutation\n");
scanf("%d",&n);
  if(flag==2){
    int a[n],b[n],c[n];
    printf("enter permutations\n");
        for(i=0;i<n;i++){
            scanf("%d",a+i);
            scanf("%d",b+i);
            c[i]=a[b[i]-1];
            printf("c:\n");
            printf("%d ",c[i]);}}
        else{
        int a[n],b[n];
        printf("enter permutation\n");
        for(i=0;i<n;i++){
            scanf("%d",a+i);
            b[a[i]-1]=i+1;
        printf("Reverse permutation:\n");
        for(i=0;i<n;i++){
            printf("%d ",b[i]);}}
return 0;}}

